        },
        {
          "$kind": "Microsoft.BeginDialog",
          "$designer": {
            "id": "tKan9D"
          },
          "activityProcessed": true,
          "dialog": "pedirNomina"
        },
        {
          "$kind": "Microsoft.BeginDialog",
          "$designer": {
            "id": "NUDxaP"
          },
          "activityProcessed": true,
          "dialog": "menu_principal"
        }
      ]
